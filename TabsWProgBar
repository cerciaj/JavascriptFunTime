      window.addEventListener("mousemove", formFilled);
      function formFilled() {
          console.log(pt1Filled());
          console.log(pt2Filled());
          console.log(pt3Filled());
          console.log(pt4Filled());
          console.log(pt5Filled());
          if (pt1Filled() == true) {
              document.getElementById("pt1but").setAttribute("style", "background-color: green;");
          } else {
              document.getElementById("pt1but").setAttribute("style", "background-color: #555;");
          }
          if (pt2Filled() == true) {
              document.getElementById("pt2but").setAttribute("style", "background-color: green;");
          } else {
              document.getElementById("pt2but").setAttribute("style", "background-color: #555;");
          }
          if (pt3Filled() == true) {
              document.getElementById("pt3but").setAttribute("style", "background-color: green;");
          } else {
              document.getElementById("pt3but").setAttribute("style", "background-color: #555;");
          }
          if (pt4Filled() == true) {
              document.getElementById("pt4but").setAttribute("style", "background-color: green;");
          } else {
              document.getElementById("pt4but").setAttribute("style", "background-color: #555;");
          }
          if (pt5Filled() == true) {
              document.getElementById("pt5but").setAttribute("style", "background-color: green;");
          } else {
              document.getElementById("pt5but").setAttribute("style", "background-color: #555;");
          }
          if (pt1Filled() == true && pt2Filled() == true && pt3Filled() == true && pt4Filled() == true && pt5Filled() == true) {
              document.getElementById("doneNotdone").innerHTML = "Submit"
          } else {
              document.getElementById("doneNotdone").innerHTML = "Save"
          }
      }
      //check part 1 filed (same for parts 1 - 5)
      function pt1Filled() {
          var fields = document.getElementById("FORM_M00140").elements;
          for (var i = 0; i < 6; i++) {
              if(fields[i].value === "") {
                  return false;
              }
         }
         return true;
      }
      function pt2Filled() {
          var fields = document.getElementById("FORM_M00140").elements;
          for (var i = 6; i < 14; i++) {
               if (fields[i].value === "") {
                  return false;
              }  
         }
         return true;
      }
      function pt3Filled() {
          var fields = document.getElementById("FORM_M00140").elements;
          for (var i = 14; i < 18; i++) {
             if (fields[i].value === "") {
                  return false;
              } 
         }
         return true
      }
      function pt4Filled() {
          var fields = document.getElementById("FORM_M00140").elements;
          for (var i = 18; i < 28; i++) {
             if (fields[i].value === "") {
                  return false;
              } 
         }
         return true
      }
      function pt5Filled() {
          var fields = document.getElementById("FORM_M00140").elements;
          for (var i = 27; i < 35; i++) {
             if (fields[i].value === "") {
                  return false;
              } 
         }
         return true
      }
      function openPage(pageName, elmnt, color) {
        // Hide all elements with class="tabcontent" by default */
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
          tabcontent[i].style.display = "none";
        }
        // Remove the background color of all tablinks/buttons
        tablinks = document.getElementsByClassName("tablink");
        for (i = 0; i < tablinks.length; i++) {
          tablinks[i].style.backgroundColor = "";
        }
        // Show the specific tab content
        document.getElementById(pageName).style.display = "block";
        // Add the specific color to the button used to open the tab content
        elmnt.style.backgroundColor = color;
      }
// Get the element with id="defaultOpen" and click on it
        document.getElementById("pt1but").click(); 
    </script>
